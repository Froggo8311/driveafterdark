{
  "name": "DocsAfterDark",
  "version": "0.1.2",
  "description": "A toggleable dark mode theme for Google Docs that uses the VCDM color scheme.",
  "author": "Raymond Wang",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://docs.google.com/document/*"
      ],
      "js": [
        "word.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "docs.css"
  ],
  "icons": {
    "16": "dad16.png",
    "48": "dad48.png",
    "128": "dad128.png"
  }
}